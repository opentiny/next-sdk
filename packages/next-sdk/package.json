{
  "name": "@opentiny/next-sdk",
  "version": "0.1.3-alpha.4",
  "type": "module",
  "license": "MIT",
  "author": "Chunhui Mo",
  "main": "index.ts",
  "module": "index.ts",
  "types": "index.ts",
  "description": "OpenTiny NEXT SDK",
  "scripts": {
    "build:all": "pnpm build:cdn && pnpm build",
    "build": "tsc",
    "build:cdn": "vite build && pnpm build:webAgent && pnpm build:webMcp && pnpm build:mcpSdk && pnpm build:zod && pnpm build:webMcpFull",
    "build:webAgent": "vite build --config vite.config.webAgent.ts",
    "build:webMcp": "vite build --config vite.config.webMcp.ts",
    "build:mcpSdk": "vite build --config vite.config.mcpSdk.ts",
    "build:zod": "vite build --config vite.config.zod.ts",
    "build:webMcpFull": "vite build --config vite.config.webMcpFull.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@opentiny/next": "^0.3.0",
    "@ai-sdk/openai": "^2.0.0",
    "@ai-sdk/deepseek": "^1.0.7",
    "@ai-sdk/provider": "^2.0.0",
    "qrcode": "^1.5.4",
    "zod": "^3.23.8",
    "ajv": "^8.17.1",
    "ai": "^5.0.9"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "@types/qrcode": "^1.5.5",
    "@types/node": "^24.3.0"
  },
  "publishConfig": {
    "access": "public",
    "module": "dist/index.js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
